----- br-srv
[srv]
hq-srv ansible_ssh_user=sshuser ansible_port=2025
br-srv ansible_ssh_user=sshuser ansible_port=2025
[routers]
hq-rtr ansible_user=net_admin ansible_password=P@ssw0rd ansible_connection=networ>
br-rtr ansible_user=net_admin ansible_password=P@ssw0rd ansible_connection=networ>
[cli]
hq-cli ansible_ssh_user=student
